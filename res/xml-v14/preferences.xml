<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"

    app:key="main_screen">

    <Preference
        app:icon="@drawable/ic_attention"
        app:key="translations_wanted"
        app:summary="Please add or update the translation for your language."
        app:title="Translations wanted !">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="https://nightclock.duckdns.org/pastt" />
    </Preference>

    <Preference
        app:icon="@drawable/ic_attention"
        app:key="upgrade_wanted"
        app:summary="@string/buy_upgrade_summary"
        app:title="@string/buy_upgrade_title">
    </Preference>

    <PreferenceScreen
        app:icon="@drawable/ic_power"
        app:key="autostart"
        app:title="@string/handle_power">

        <SwitchPreference
            app:defaultValue="false"
            app:key="handle_power"
            app:title="@string/handle_power" />

        <PreferenceCategory
            key="category_handle_power_autostart_time"
            app:dependency="handle_power"
            app:title="@string/category_handle_power_autostart_time">

            <de.firebirdberlin.preference.TimeRangePreference
                xmlns:timerange="timerange"
                app:defaultValue="0"
                app:dependency="handle_power"
                app:key="autostart_time_range"
                timerange:end_text="@string/autostart_timerange_label_end"
                timerange:key_suffix_end="_end"
                timerange:key_suffix_start="_start"
                timerange:start_text="@string/autostart_timerange_label_start" />
        </PreferenceCategory>

        <PreferenceCategory
            key="category_handle_power"
            app:dependency="handle_power"
            app:title="@string/category_handle_power">

            <CheckBoxPreference
                app:defaultValue="true"
                app:key="handle_power_ac"
                app:title="@string/handle_power_ac" />

            <CheckBoxPreference
                app:defaultValue="false"
                app:key="handle_power_usb"
                app:title="@string/handle_power_usb" />

            <CheckBoxPreference
                app:defaultValue="false"
                app:key="handle_power_wireless"
                app:title="@string/handle_power_wireless" />

            <CheckBoxPreference
                app:defaultValue="true"
                app:key="handle_power_desk"
                app:title="@string/handle_power_desk" />

            <CheckBoxPreference
                app:defaultValue="false"
                app:key="handle_power_car"
                app:title="@string/handle_power_car" />

        </PreferenceCategory>

        <PreferenceCategory
            app:title="@string/handle_auto_stop">

            <SwitchPreference
                app:defaultValue="true"
                app:key="handle_power_disconnection"
                app:summary="@string/handle_power_disconnection_summary"
                app:title="@string/handle_power_disconnection" />

            <SwitchPreference
                app:defaultValue="true"
                app:key="handle_power_disconnection_at_time_range_end"
                app:title="@string/handle_power_disconnection_at_time_range_end" />

            <ListPreference
                app:defaultValue="-1"
                app:entries="@array/batteryTimeout"
                app:entryValues="@array/batteryTimeoutValues"
                app:key="batteryTimeout"
                app:title="@string/batteryTimeoutDescription" />

        </PreferenceCategory>

        <Preference
            app:icon="@drawable/ic_googleplay"
            app:key="purchaseActions"
            app:title="@string/purchaseActions" />

        <PreferenceCategory
            app:icon="@drawable/ic_expert"
            app:key="expert_screen"
            app:enabled="false"
            app:title="@string/always_on">

            <SwitchPreference
                app:defaultValue="false"
                app:key="standbyEnabledWhileDisconnected"
                app:summary="@string/standbyEnabledWhileDisconnectedSummary"
                app:title="@string/standbyEnabledWhileDisconnectedTitle" />

            <de.firebirdberlin.preference.TimeRangePreference
                xmlns:timerange="timerange"
                app:defaultValue="0"
                app:dependency="standbyEnabledWhileDisconnected"
                app:key="always_on_time_range"
                app:title="@string/standbyEnabledWhileDisconnectedTimeRangeTitle"
                timerange:end_text="@string/autostart_timerange_label_end"
                timerange:key_suffix_end="_end"
                timerange:key_suffix_start="_start"
                timerange:start_text="@string/autostart_timerange_label_start" />

            <CheckBoxPreference
                app:defaultValue="false"
                app:key="standbyEnabledWhileDisconnectedScreenUp"
                app:dependency="standbyEnabledWhileDisconnected"
                app:title="@string/standbyEnabledWhileDisconnectedScreepUpTitle" />

            <de.firebirdberlin.preference.InlineSeekBarPreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="20"
                app:key="alwaysOnBatteryLevel"
                app:dependency="standbyEnabledWhileDisconnected"
                android:max="90"
                app:title="@string/alwaysOnBatteryLevel"
                seekbar:interval="5"
                seekbar:min="0"
                seekbar:unitsLeft=""
                seekbar:unitsRight="%"/>

            <SwitchPreference
                app:defaultValue="false"
                app:key="autostartForNotifications"
                app:summary="@string/autostartForNotificationsSummary"
                app:title="@string/autostartForNotificationsTitle" />
        </PreferenceCategory>

        <PreferenceCategory
            app:enabled="false"
            app:icon="@drawable/ic_expert"
            app:key="scheduled_autostart_screen"
            app:title="@string/scheduledAutoStart">

            <SwitchPreference
                app:defaultValue="false"
                app:key="scheduledAutoStartEnabled"
                app:summary="@string/scheduledAutoStartEnabledDesc"
                app:title="@string/scheduledAutoStartEnabledTitle" />

            <de.firebirdberlin.preference.TimeRangePreference
                xmlns:timerange="timerange"
                app:defaultValue="0"
                app:dependency="scheduledAutoStartEnabled"
                app:key="scheduledAutoStartTimeRange"
                app:title="@string/scheduledAutoStartTimeRange"
                timerange:end_text="@string/autostart_timerange_label_end"
                timerange:key_suffix_end="_end"
                timerange:key_suffix_start="_start"
                timerange:start_text="@string/autostart_timerange_label_start" />

            <CheckBoxPreference
                app:defaultValue="true"
                app:dependency="scheduledAutoStartEnabled"
                app:key="scheduledAutoStartChargerRequired"
                app:title="@string/scheduledAutoStartChargerRequired" />

        </PreferenceCategory>
    </PreferenceScreen>

    <PreferenceScreen
        app:icon="@drawable/ic_appearance"
        app:key="colors_screen"
        app:title="@string/appearance">

        <PreferenceCategory
            app:title="@string/appearance">

            <Preference
                app:icon="@drawable/ic_googleplay"
                app:key="purchaseDesignPackage"
                app:title="@string/purchaseWeatherData" />

            <ListPreference
                app:defaultValue="0"
                app:entries="@array/clockLayout"
                app:entryValues="@array/clockLayoutValues"
                app:key="clockLayout"
                app:title="@string/clock_layout" />

            <com.firebirdberlin.nightdream.ui.ClockLayoutPreviewPreference
                android:layout_gravity="center"
                android:gravity="center"
                app:key="clockLayoutPreview"
                app:selectable="false"
                app:title="@string/preview" />

            <com.firebirdberlin.nightdream.ui.ColorSelectionPreference
                android:descendantFocusability="afterDescendants"
                app:key="colorSelection"
                app:selectable="true"
                app:title="@string/colors"
                />

            <SwitchPreference
                app:defaultValue="true"
                app:key="showDate"
                app:title="@string/show_date" />

            <com.firebirdberlin.nightdream.DateFormatPreference
                app:dependency="showDate"
                app:key="dateFormat"
                app:title="@string/date_format" />

            <com.firebirdberlin.nightdream.DateFormatPreference
                xmlns:custom="http://schemas.android.com/apk/res-auto"
                app:key="timeFormat"
                app:title="@string/timeFormat"
                custom:formatType="time" />

        </PreferenceCategory>

        <PreferenceCategory app:title="@string/background">
            <ListPreference
                app:defaultValue="1"
                app:entries="@array/backgroundModes"
                app:entryValues="@array/backgroundModeValues"
                app:key="backgroundMode"
                app:title="@string/background" />

            <Preference
                app:key="chooseBackgroundImage"
                app:title="@string/background_image"/>

            <CheckBoxPreference
                app:defaultValue="true"
                app:key="hideBackgroundImage"
                app:summary="@string/hide_background_image_in_night_mode_summary"
                app:title="@string/hide_background_image_in_night_mode" />

        </PreferenceCategory>

        <PreferenceCategory
            app:key="category_brightness"
            app:title="@string/brightness">

            <SwitchPreference
                app:defaultValue="false"
                app:key="autoBrightness"
                app:title="@string/auto_brightness" />

            <de.firebirdberlin.preference.InlineSeekBarPreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="80"
                app:key="brightness_offset"
                android:max="100"
                app:title="@string/brightness_offset"
                seekbar:interval="1"
                seekbar:min="-100"
                seekbar:unitsLeft=""
                seekbar:unitsRight=""/>
        </PreferenceCategory>

    </PreferenceScreen>

    <PreferenceScreen
        app:icon="@drawable/ic_expert"
        app:title="@string/behaviour">

        <SwitchPreference
            app:defaultValue="false"
            app:key="doubleTapToFinish"
            app:summary="@string/doubleTapToFinishSummary"
            app:title="@string/doubleTapToFinishTitle" />

        <SwitchPreference
            app:defaultValue="true"
            app:key="restlessMode"
            app:title="@string/restless_mode" />

        <SwitchPreference
            app:defaultValue="true"
            app:key="persistentBatteryValueWhileCharging"
            app:summary="@string/persistent_battery_value_while_charging_summary"
            app:title="@string/persistent_battery_value_while_charging" />

        <SwitchPreference
            app:defaultValue="true"
            app:key="showBatteryWarning"
            app:summary="@string/showBatteryWarningSummary"
            app:title="@string/showBatteryWarningTitle" />

        <ListPreference
            app:defaultValue="-1"
            app:entries="@array/screenOrientation"
            app:entryValues="@array/screenOrientationValues"
            app:key="screenOrientation"
            app:title="@string/screenOrientation" />

        <SwitchPreference
            app:defaultValue="false"
            app:key="clockLayoutMirrorText"
            app:title="@string/clock_layout_mirror_text" />

        <Preference
            app:icon="@drawable/ic_googleplay"
            app:key="purchaseActions2"
            app:title="@string/purchaseActions" />

        <SwitchPreference
            app:defaultValue="false"
            app:enabled="false"
            app:key="activateDoNotDisturb"
            app:summary="@string/activateDoNotDisturbSummary"
            app:title="@string/activateDoNotDisturb" />

        <SwitchPreference
            app:defaultValue="false"
            app:key="useDeviceLock"
            app:enabled="false"
            app:summary="@string/useDeviceLockDescription"
            app:title="@string/useDeviceLock" />

    </PreferenceScreen>

    <PreferenceScreen
        app:icon="@drawable/ic_nightmode"
        app:key="night_mode_screen"
        app:title="@string/night_mode">

        <PreferenceCategory
            app:key="category_night_mode"
            app:title="@string/night_mode">

            <ListPreference
                app:defaultValue="1"
                app:entries="@array/nightModeActivationValues"
                app:entryValues="@array/nightModeActivationModes"
                app:key="nightModeActivationMode"
                app:summary="%s"
                app:title="@string/night_mode_activation_mode" />

            <de.firebirdberlin.preference.TimeRangePreference
                xmlns:timerange="timerange"
                app:defaultValue="0"
                app:enabled="false"
                app:key="nightmode_timerange"
                app:title="@string/night_mode_timerange_title"
                timerange:end_text="@string/night_mode_timerange_label_end"
                timerange:key_suffix_end="_end"
                timerange:key_suffix_start="_start"
                timerange:start_text="@string/night_mode_timerange_label_start" />

            <CheckBoxPreference
                app:defaultValue="false"
                app:key="Night.muteRinger"
                app:title="@string/mute_ringer" />

            <SwitchPreference
                app:defaultValue="false"
                app:key="ambientNoiseDetection"
                app:summary="@string/ambient_noise_summary"
                app:title="@string/ambient_noise" />

            <SwitchPreference
                app:defaultValue="false"
                app:key="allow_screen_off"
                app:summary="@string/allow_screen_off_summary"
                app:title="@string/allow_screen_off" />

            <de.firebirdberlin.preference.InlineSeekBarPreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="20"
                app:dependency="allow_screen_off"
                app:key="reactivate_on_ambient_light_value"
                android:max="30"
                app:summary="@string/reactivate_on_ambient_light_value_summary"
                app:title="@string/reactivate_on_ambient_light_value"
                seekbar:interval="1"
                seekbar:min="5"
                seekbar:unitsLeft=""
                seekbar:unitsRight="lux"/>

            <CheckBoxPreference
                app:defaultValue="false"
                app:dependency="allow_screen_off"
                app:key="reactivate_screen_on_noise"
                app:title="@string/reactivate_screen_on_noise" />

            <de.firebirdberlin.preference.InlineSeekBarPreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="5"
                app:key="NoiseSensitivity"
                android:max="10"
                app:title="@string/noise_sensitivity"
                seekbar:interval="1"
                seekbar:min="0"
                seekbar:unitsLeft=""
                seekbar:unitsRight=""/>
        </PreferenceCategory>
    </PreferenceScreen>

    <PreferenceScreen
            app:icon="@drawable/ic_alarm_clock"
            app:key="alarm_screen"
            app:title="@string/alarm">

            <SwitchPreference
                app:defaultValue="true"
                app:key="useInternalAlarm"
                app:title="@string/use_internal_alarm" />

            <SwitchPreference
                app:defaultValue="false"
                app:dependency="useInternalAlarm"
                app:key="useAlarmSwipeGesture"
                app:title="@string/useAlarmSwipeGesture" />

            <com.firebirdberlin.nightdream.ui.AlarmVolumePreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="3"
                app:dependency="useInternalAlarm"
                app:key="alarmVolume"
                app:title="@string/alarmVolume" />

            <CheckBoxPreference
                app:defaultValue="true"
                app:dependency="useInternalAlarm"
                app:key="alarmFadeIn"
                app:title="@string/alarmFadeIn" />

            <de.firebirdberlin.preference.InlineSeekBarPreference
                xmlns:seekbar="http://schemas.android.com/apk/lib/android"
                app:defaultValue="5"
                app:dependency="useInternalAlarm"
                app:key="snoozeTimeInMinutes"
                android:max="60"
                app:title="@string/snoozeTime"
                seekbar:interval="1"
                seekbar:min="5"
                seekbar:unitsLeft=""
                seekbar:unitsRight="min"/>

            <PreferenceCategory
                app:key="category_radio_stream"
                app:title="@string/radio_stream">

                <SwitchPreference
                    app:defaultValue="false"
                    app:key="radioStreamMusicIsAllowedForAlarms"
                    app:title="@string/radio_stream_music_allowed" />

                <SwitchPreference
                    app:defaultValue="true"
                    app:key="radioStreamRequireWiFi"
                    app:summary="@string/radio_stream_require_wifi_desc"
                    app:title="@string/radio_stream_require_wifi" />

                <SwitchPreference
                    app:defaultValue="false"
                    app:key="radioStreamActivateWiFi"
                    app:summary="@string/radio_stream_activate_wifi_desc"
                    app:title="@string/radio_stream_activate_wifi" />
            </PreferenceCategory>
        </PreferenceScreen>

        <PreferenceScreen
            app:icon="@drawable/ic_cloud"
            app:key="weather_screen"
            app:title="@string/weather">

            <Preference
                app:icon="@drawable/ic_googleplay"
                app:key="purchaseWeatherData"
                app:title="@string/purchaseWeatherData"/>

            <SwitchPreference
                app:defaultValue="false"
                app:enabled="false"
                app:key="showWeather"
                app:title="@string/show_weather" />

            <SwitchPreference
                app:defaultValue="true"
                app:dependency="showWeather"
                app:key="showTemperature"
                app:title="@string/show_temperature" />

            <ListPreference
                app:defaultValue="1"
                app:dependency="showTemperature"
                app:entries="@array/temperatureUnit"
                app:entryValues="@array/temperatureUnitValues"
                app:key="temperatureUnit"
                app:title="@string/temperatureUnit" />

            <SwitchPreference
                app:defaultValue="false"
                app:dependency="showWeather"
                app:key="showWindSpeed"
                app:title="@string/show_wind_speed" />

            <ListPreference
                app:defaultValue="1"
                app:dependency="showWindSpeed"
                app:entries="@array/speedUnit"
                app:entryValues="@array/speedUnitValues"
                app:key="speedUnit"
                app:title="@string/speedUnit" />

            <com.firebirdberlin.openweathermapapi.CityIDPreference
                xmlns:owmapi="owmapi"
                app:dependency="showWeather"
                app:icon="@drawable/ic_search"
                app:key="weatherCityID"
                app:title="@string/weather_city_id_title"
                owmapi:textClear="@string/clear"
                owmapi:textSummary=""
                />

            <ListPreference
                app:defaultValue="0"
                app:entries="@array/weatherProvider"
                app:entryValues="@array/weatherProviderValues"
                app:key="weatherProvider"
                app:title="@string/weatherProvider" />

            <com.firebirdberlin.nightdream.ui.WeatherPreviewPreference
                app:dependency="showWeather"
                app:key="weatherPreview"
                app:title="@string/weather_info_title" />

        </PreferenceScreen>

        <Preference
            app:icon="@drawable/ic_menu"
            app:key="startNotificationService"
            app:title="@string/buttonAccessibilitySettings" />

        <PreferenceScreen
            app:icon="@drawable/ic_about"
            app:key="about_screen"
            app:title="@string/about">

            <de.firebirdberlin.preference.VersionPreference
                app:icon="@drawable/ic_clock"
                app:key="version"
                app:selectable="true"
                app:title="NightDream">
                <intent
                    android:action="android.intent.action.VIEW"
                    android:data="https://play.google.com/store/apps/details?id=com.firebirdberlin.nightdream"
                    />
            </de.firebirdberlin.preference.VersionPreference>

            <Preference
                app:icon="@drawable/ic_faq"
                app:title="FAQ">
                <intent
                    android:action="android.intent.action.VIEW"
                    android:data="https://github.com/firebirdberlin/nightdream/wiki" />

            </Preference>

            <Preference
                app:icon="@drawable/ic_share"
                app:key="recommendApp"
                app:title="@string/recommend"/>

            <Preference
                app:icon="@drawable/ic_undo"
                app:key="reset_to_defaults"
                app:title="@string/reset_to_defaults"/>

            <Preference
                app:icon="@drawable/ic_delete"
                app:key="uninstallApp"
                app:title="@string/uninstall"/>

            <Preference
                app:title="@string/titlePrivacyPolicy">
                <intent
                    android:action="android.intent.action.VIEW"
                    android:data="http://nightclock.duckdns.org/apps/NightDream/privacy.html"
                    />
            </Preference>

            <PreferenceScreen app:title="@string/credits">
                <Preference
                    app:enabled="true"
                    app:key="credits"
                    app:selectable="false"
                    app:summary="stefan.fruhner@googlemail.com"
                    app:title="Stefan Fruhner (2018)" />

                <Preference
                    app:enabled="true"
                    app:key="credits_pr"
                    app:selectable="false"
                    app:summary="Martin Poschmann"
                    app:title="@string/credits_pull_requests" />

                <Preference
                    app:enabled="true"
                    app:key="translations"
                    app:selectable="false"
                    app:summary="
ar - M. Hakam et. al.\n
cs - Tomáš Holub\n
